library(tidyverse)
df<-read.csv("../datasets/BreastTissue.csv",sep=";",dec=",")
df<-read.csv("../datasets/BreastTissue.csv",sep=";",dec=",")
df<-read.csv("BreastTissue.csv",sep=";",dec=",")
colnames(df)[1]<-"Class"
#build target variable as factor
df$Class <- as.factor(df$Class)
levels(df$Class)[levels(df$Class) %in% c("fad", "gla", "mas")] <- "other"
head(df)
unique(df$Class)
library(caret)
index <- createDataPartition(df$Class, p = .70, list = FALSE)
train.data <- df[index,]
test.data <- df[-index,]
library(nnet)
train.data$Class <- relevel(train.data$Class, ref = "adi")
# Training the multinomial model
multinom_model <- multinom(Class ~ ., data = train.data)
# Checking the model
summary(multinom_model)
# Predicting the values for train dataset
train.data$ClassPredicted <- predict(multinom_model, newdata = train.data, "class")
# Building classification table
tab <- table(train.data$Class, train.data$ClassPredicted)
tab
# Calculating accuracy - sum of diagonal elements divided by total obs
round((sum(diag(tab))/sum(tab))*100,2)
# Predicting the class for test dataset
test.data$ClassPredicted <- predict(multinom_model, newdata = test.data, "class")
# Building classification table
tab <- table(test.data$Class, test.data$ClassPredicted)
tab
# Calculating accuracy - sum of diagonal elements divided by total obs
round((sum(diag(tab))/sum(tab))*100,2)
